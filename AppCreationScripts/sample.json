{
    "Sample": {
      "Title": "An Angular single-page application that authenticates users with Azure AD and calls a protected ASP.NET Core web API",
      "Level": 200,
      "Client": "TodoListSPA",
      "Service": "TodoListAPI",
      "RepositoryUrl": "ms-identity-javascript-angular-spa-aspnetcore-webapi",
      "Endpoint": "AAD v2.0"
    },
    "AADApps": [
      {
        "Id": "service",
        "Name": "TodoListAPI",
        "Kind": "WebApi",
        "Audience": "AzureADandPersonalMicrosoftAccount",
        "HomePage": "https://localhost:44351/api/todolist/",
        "ManualSteps": [
          {
            "Comment": "Navigate to the Manifest page and change 'signInAudience' to 'AzureADandPersonalMicrosoftAccount'."
          }
        ]
      },
      {
        "Id": "client",
        "Name": "TodoListSPA",
        "Kind": "SinglePageApplication",
        "Audience": "AzureADandPersonalMicrosoftAccount",
        "HomePage": "http://localhost:4200/",
        "ReplyUrls": "http://localhost:4200/",
        "RequiredResourcesAccess": [
          {
            "Resource": "service",
            "DelegatedPermissions": [ "access_as_user" ]
          }
        ],
        "ManualSteps": [
          {
            "Comment": "Navigate to the Manifest page and change 'signInAudience' to 'AzureADandPersonalMicrosoftAccount'."
          }
        ]
      }
    ],
    "CodeConfiguration": [
      {
        "App": "service",
        "SettingKind": "JSon",
        "SettingFile": "\\..\\TodoListApplication\\TodoListAPI\\appsettings.json",
        "Mappings": [
          {
            "key": "Domain",
            "value": "$tenantName"
          },
          {
            "key": "TenantId",
            "value": "$tenantId"
          },
          {
            "key": "ClientId",
            "value": "service.AppId"
          }
        ]
      },
  
      {
        "App": "client",
        "SettingKind": "JSon",
        "SettingFile": "\\..\\TodoListApplication\\TodoListSPA\\src\\app\\app-config.json",
        "Mappings": [
          {
            "key": "clientId",
            "value": "client.AppId"
          },
          {
            "key": "redirectUri",
            "value": "client.HomePage"
          },
          {
            "key": "postLogoutRedirectUri",
            "value": "client.HomePage"
          },
          {
            "key": "resourceUri",
            "value": "service.HomePage"
          },
          {
            "key": "resourceScope",
            "value": "service.Scope"
          }
        ]
      }
    ]
  }